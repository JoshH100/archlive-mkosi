# vim: set ft=systemd
[Distribution]
Distribution=arch

[Output]
Hostname=archstrap
Format=directory

[Packages]
Packages=arch-install-scripts,base,tar,dosfstools,git,lvm2,base,zsh,python,ansible,coreutils,vim,parted,dhclient,archiso,mkinitcpio-nfs-utils,openssh,open-vm-tools,wget,efibootmgr
Cache=/var/cache/pacman/pkg
ExtraTrees=./root
WithNetwork=yes

[Partitions]
RootSize=2500M
